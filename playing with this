package src.main.java.practise;
public class TheDemo {
    int a,b;
    public void get(int x, int y){
        this.a=x;
        this.b=y;
    }
        public void pur(){
            System.out.println("a="+a);
            System.out.println("b="+b);
        
    }
}
class Demo{
    public static void main(String[] args){
        TheDemo ob1=new TheDemo();
        ob1.get(10,20);
        ob1.pur();
    }
}
