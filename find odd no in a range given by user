package src.main.java.practise;
//wap to print all odd no in the range 1 to n
import java.util.*;
public class OddNo{
    public static void main(String[] args){

        Scanner sc=new Scanner(System.in);
        System.out.println("enter the range");
        int n=sc.nextInt();
        for(int i=1;i<=n;i++){
          if(i%2!=0){
            System.out.println(i+"is a odd no");
          }


        }
        sc.close();





    }
}
